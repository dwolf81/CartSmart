{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (Web API)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/src/CartSmart.Web/CartSmart.API/bin/Debug/net8.0/CartSmart.API.dll",
        "args": [],
        "cwd": "${workspaceFolder}/src/CartSmart.Web/CartSmart.API",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "sourceFileMap": {
          "/Users": "${workspaceFolder}/Users"
        }
      }
    ]
  }
  